{
	"job": {
		"content": [{
			"reader": {
				"parameter": {
					"topic": "GraphNodeUpdated",
					"mode": "earliest-offset",
					"codec": "json",
					"encoding": "UTF-8",
					"blankIgnore": false,
					"consumerSettings": {
						"bootstrap.servers": "kafka001:9092",
						"group.id" : "data_pipeline2",
						"enable.auto.commit": "true",
						"auto.commit.interval.ms" : "1000"
					}
				},
				"name": "kafkareader"

			},
			"writer": {
				"parameter": {
					"connection": [{
						"jdbcUrl": "jdbc:postgresql://localhost:26257/saga_kafka?sslmode=require",
						"table": [
							"GraphNodeUpdated"
						]
					}],
					"username": "yoda_readwrite",
					"password": "yoda_123456",
					"column": [{
							"name": "id",
							"type": "VARCHAR"
						},
						{
							"name": "nodeType",
							"type": "VARCHAR"
						},
						{
							"name": "eventId",
							"type": "VARCHAR"
						},
						{
							"name": "operatorId",
							"type": "VARCHAR"
						},
						{
							"name": "tags",
							"type": "VARCHAR"
						},
						{
							"name": "properties",
							"type": "VARCHAR"
						},
						{       
							"name": "message",
							"type": "VARCHAR"
						}
					],
					"writeMode": "update",
					"batchSize": 1,
					"preSql": [],
					"postSql": []
				},
				"name": "postgresqlwriter"
			}
		}],
		"setting": {
			"restore": {
				"isRestore": false,
				"isStream": true
			},
			"speed": {
				"readerChannel": 1,
				"writerChannel": 1
			},
			"errorLimit" : {
 				"record": 1,
 				"percentage": 100.0
			},
			"dirty": {},
			"log": {}
		}
	}
}
